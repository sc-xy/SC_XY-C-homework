#include<stdio.h> 
#include<string.h>
int main(){
	int n,a[600],b[600],i,j,c[1100],n1,n2,s;
	char num1[600],num2[600];
	scanf("%d",&n); 
	while(n--)
	{
		scanf("%s %s",num1,num2);
		n1=strlen(num1),n2=strlen(num2);
		for(s=0;s<600;s++)a[s]=0,b[s]=0;
		for(i=0,j=n1-1;i<n1;i++,j--)a[i]=num1[j]-'0';
		for(i=0,j=n2-1;i<n2;i++,j--)b[i]=num2[j]-'0';
		for(s=0;s<1100;s++)c[s]=0;
		for(i=0;i<n1;i++)
		for(j=0;j<n2;j++)c[i+j]+=a[i]*b[j];
		for(s=0;s<i+j;s++)
		{
			if(c[s]>=10)
			{
				c[s+1]+=c[s]/10;
				c[s]%=10;
			}
		}
		for(s=i+j;c[s]==0;s--);
		for(;s>=0;s--)printf("%d",c[s]);
		printf("\n");
	}
	return 0;
}
