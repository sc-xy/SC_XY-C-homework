#include<iostream>
using namespace std;
int a[100100],n,b[100010];
long long sum=0;
void msort(int left,int right)
{
    if(left>=right)return;
    int mid=(left+right)/2;
    msort(left,mid);
    msort(mid+1,right);//划分为小区间部分
    int i=left,k=left,j=mid+1;//索引两个区间头
    while(i<=mid&&j<=right)
    {
        if(a[i]>a[j])
        {
            b[k++]=a[j++];
            sum+=mid+1-i;
        }
        else b[k++]=a[i++];
    }
    while(i<=mid)b[k++]=a[i++];//如果i没有越界，插入完全
    while(j<=right)b[k++]=a[j++];//如果j没有越界，插入完全
    for(i=left;i<=right;i++)a[i]=b[i];//进行归并
}
int main()
{
    int i;
    cin>>n;
    for(i=0;i<n;i++)cin>>a[i];
    msort(0,n-1);
    cout<<sum<<endl;
    return 0;
}
