#include<iostream>
#include<string.h>
using namespace std;
int main()
{
    long long a[35660];
    memset(a,0,sizeof(a));
    a[0]=1;
    int c,i,b;
    cin>>c;
    if(c<2)cout<<1;
    else
    {
        for(i=2;i<=c;i++)
        {
            for(b=35659;a[b]==0;b--);
            for(;b>=0;b--)a[b]=i*a[b];
            for(b=0;b<35659;b++)
            {
                if(a[b]>=10)
                {
                    a[b+1]+=a[b]/10;
                    a[b]%=10;
                }
            }
        }
        for(b=35659;a[b]==0;b--);
        for(;b>=0;b--)cout<<a[b];
    }
    return 0;
}

