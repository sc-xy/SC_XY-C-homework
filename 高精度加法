#include<iostream>
#include<string.h>
using namespace std;
int main()
{
    char a[300],b[300];
    int ad[300],add[300],c,ans[300],q,w;
    memset(a,'\0',sizeof(a));
    memset(b,'\0',sizeof(b));
    memset(ad,0,sizeof(ad));
    memset(add,0,sizeof(add));
    memset(ans,0,sizeof(ans));
    scanf("%s %s",a,b);
    for(c=0,q=strlen(a)-1;q>=0;q--,c++)ad[c]=a[q]-'0';
    for(c=0,q=strlen(b)-1;q>=0;q--,c++)add[c]=b[q]-'0';
    q=strlen(a)>strlen(b)?strlen(a):strlen(b);    
    for(c=0;c<q;c++)ans[c]=ad[c]+add[c];
    for(c=0;c<=q;c++)
    {
        if(ans[c]>=10)
        {
            ans[c+1]+=ans[c]/10;
            ans[c]%=10;
        }
    }
    for(c=q;ans[c]==0;c--);
    for(;c>=0;c--)cout<<ans[c];
    return 0;
}

