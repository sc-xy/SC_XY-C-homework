#include<iostream>
#include<string.h>
using namespace std;
int a[100],b[100];
void ch(int a[],int b[],int n,int m)
{
    if(n==m)
    {
        cout<<"--> ";
        for(int i=0;i<n;i++)
        {
            if(b[i]==1)cout<<a[i]<<" ";
        }
        cout<<endl;
        return;
    }
    b[n]=1;
    ch(a,b,n+1,m);
    b[n]=0;
    ch(a,b,n+1,m);
}
int main()
{
    int n,i,x;
    scanf("%d",&n);
    while(n--)
    {
        scanf("%d",&x);
        i=x;
        while(i--)
        {
            scanf("%d",&a[x-i-1]);
        }
        ch(a,b,0,x);
    }
    return 0;
}
